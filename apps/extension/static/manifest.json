{
	"name": "My first Svelte Extension",
	"description": "A browser extension made with Svelte Kit",
	"version": "1.0.0",
	"manifest_version": 3,
	"permissions": ["activeTab", "scripting"],
	"action": {
		"default_title": "Svelte Extension",
		"default_icon": "favicon.png",
		"default_popup": "index.html"
	},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["contentScript.js"]
		}
	]
}
